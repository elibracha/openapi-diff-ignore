project: sample-service
paths:
  /pet/**, /store/**, /user/**:
    get:
      parameters:
        - username
        - password

    post:
      parameters:
        - username
        - password
      request:
        content:
          application/octet-stream: $
          application/x-www-form-urlencoded:
            schema:
              properties:
                - orderId
      response:
        200:
          content:
            application/json:
              schema:
                properties:
                  - petId
                  - quantity
                  - shipDate
                  - complete
      security:
        petstore_auth:
          - 'write:pets'
          - 'write:pests'
          - 'read:pets'


    put: $
